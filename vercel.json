{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*\\.(js|css|ico|png|jpg|svg|woff|woff2|ttf|otf|webp)$)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}