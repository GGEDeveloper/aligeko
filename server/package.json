{
  "name": "alitools-b2b-server",
  "version": "1.0.0",
  "description": "Backend para AliTools B2B E-commerce",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed:all": "sequelize-cli db:seed:all",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all",
    "db:seed:prod": "node dist/scripts/seed-db.js",
    "db:migrate:prod": "node dist/scripts/migrate-db.js",
    "db:test-indexes": "babel-node src/scripts/test-indexes.js",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed:all",
    "vercel-build": "npm run build && npm run db:migrate:prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "test:geko": "jest --config=jest.config.js tests/integration/geko-api.test.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "b2b",
    "ecommerce",
    "alitools",
    "api"
  ],
  "author": "AliTools Dev Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.2",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.3",
    "sequelize": "^6.37.1",
    "speakeasy": "^2.0.0",
    "umzug": "^3.7.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.0",
    "babel-jest": "^29.7.0",
    "chai": "^4.5.0",
    "jest": "^29.7.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.2",
    "sinon": "^15.2.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4"
  }
}
